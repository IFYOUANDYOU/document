}

.loadtext .load:nth-child(5) {
  -webkit-animation-delay: 0.6s;
          animation-delay: 0.6s;
}

@keyframes loading {
  0%,
  60%,
  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
  }
  30% {
    -webkit-transform: scaleY(3);
            transform: scaleY(3);
  }
}

@-webkit-keyframes loading {
